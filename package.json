{
  "name": "runnermanager",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "doctor": "expo doctor",
    "eject": "expo eject",
    "publish": "expo publish",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "prebuild": "expo prebuild",
    "start:clean": "expo start -c",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "jest",
    "check-format": "prettier --check .",
    "check-types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "check-lint": "eslint . ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install",
    "lint-staged": {
      "*.{js,jsx,ts,tsx}": [
        "prettier --write .",
        "eslint --fix",
        "git add ."
      ]
    }
  },
  "lint-stages": {
    "**/*": "prettier --write --ingore-unknown"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-community/geolocation": "^3.2.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.5",
    "@rneui/base": "^0.0.0-edge.2",
    "@rneui/themed": "^0.0.0-edge.2",
    "dotenv": "^16.4.5",
    "eslint-config-airbnb": "^19.0.4",
    "expo": "~51.0.8",
    "expo-location": "^17.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "^0.25.14",
    "formik": "^2.4.6",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-google-places-autocomplete": "^2.5.6",
    "react-native-maps": "^1.14.0",
    "react-native-maps-directions": "^1.9.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^9.1.2",
    "tailwind-react-native-classnames": "^1.5.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.2.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "3.2.5",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add ."
    ]
  },
  "private": true
}
